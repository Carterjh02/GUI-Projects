package Pck1;

import java.util.ArrayList;

public class Utility extends MainClass{

	ArrayList<String> Chicken;
	ArrayList<String> Beef;
	ArrayList<String> Pork;
	ArrayList<String> Sides;
	
	public void randomChicken() {
		for(int i = 0; i < Chicken.size(); i++) {
			int index = (int)(Math.random() * Chicken.size());
			System.out.println(Chicken.get(index));
		}
	}
}
